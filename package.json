{
  "name": "igloo-bering",
  "version": "0.1.0",
  "description": "Backend for the igloo platform",
  "scripts": {
    "dev": "nodemon ./server.js --exec babel-node --inspect -e js,graphql,env",
    "start": "npm run compile && npm run sequelize db:migrate && node ./compiled/server.js",
    "compile": "babel server.js app.js s3helpers.js shared.js admin.js dashboard.js -d compiled --source-maps && babel postgresql -d compiled/postgresql --source-maps && babel redis -d compiled/redis --source-maps && babel graphql -d compiled/graphql --source-maps && babel dataloaders -d compiled/dataloaders --source-maps",
    "test": "jest --c=jest.config.json",
    "posttest": "npm run docs",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "prettier **/*.js --write",
    "precommit": "lint-staged",
    "sequelize": "sequelize --config ./postgresql/config/config.json --models-path ./postgresql/models --migrations-path ./postgresql/migrations --seeders-path ./postgresql/seeders",
    "bill": "babel-node ./monthlyBilling",
    "docs": "./node_modules/.bin/jsdoc -c jsdoc.json"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ],
    "*.graphql": [
      "prettier --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IglooCloud/IglooBering"
  },
  "keywords": [],
  "author": "Igloo",
  "bugs": {
    "url": "https://github.com/IglooCloud/IglooBering/issues"
  },
  "homepage": "https://github.com/IglooCloud/IglooBering",
  "dependencies": {
    "@babel/plugin-transform-runtime": "^7.3.4",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "admin-bro": "^0.5.0",
    "admin-bro-expressjs": "^0.1.5",
    "admin-bro-sequelizejs": "^0.1.9",
    "apollo-server-express": "^1.0.2",
    "aws-sdk": "^2.229.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chalk": "^2.3.0",
    "connect-busboy": "^0.0.2",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "dataloader": "^1.4.0",
    "docdash": "^1.0.3",
    "dotenv": "^4.0.0",
    "express": "4.16.0",
    "express-jwt": "^5.3.0",
    "express-session": "^1.15.6",
    "fido2-lib": "^2.1.1",
    "fido2-lib-clone": "^2.3.0",
    "graphql-custom-directive": "^0.2.0",
    "graphql-subscriptions": "^0.5.5",
    "graphql-tools": "4.0.3",
    "graphql-tools-types": "1.1.16",
    "javascript-fortuna": "^1.0.8",
    "jsdoc": "^3.5.5",
    "jwt-simple": "^0.5.1",
