{
  "name": "igloo-bering",
  "version": "0.1.0",
  "description": "Backend for the igloo platform",
  "scripts": {
    "dev": "nodemon ./server.js --exec babel-node --inspect -e js,graphql,env",
    "start":
      "npm run build && npm run sequelize db:migrate && node ./build/server.js",
    "build":
      "babel server.js app.js s3helpers.js shared.js -d build --source-maps && babel postgresql -d build/postgresql --source-maps && babel graphql -d build/graphql --source-maps && babel dataloaders -d build/dataloaders --source-maps",
    "test": "jest --c=jest.config.json",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "lint": "prettier **/*.js --write",
    "precommit": "lint-staged",
    "sequelize":
      "sequelize --config ./postgresql/config/config.json --models-path ./postgresql/models --migrations-path ./postgresql/migrations --seeders-path ./postgresql/seeders",
    "bill": "babel-node ./monthlyBilling"
  },
  "lint-staged": {
    "*.js": ["prettier --write", "git add"],
    "*.json": ["prettier --write", "git add"],
    "*.graphql": ["prettier --write", "git add"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/IglooCloud/IglooBering"
  },
  "keywords": [],
  "author": "Igloo",
  "bugs": {
    "url": "https://github.com/IglooCloud/IglooBering/issues"
  },
  "homepage": "https://github.com/IglooCloud/IglooBering",
  "dependencies": {
    "apollo-server-express": "^1.0.2",
    "aws-sdk": "^2.229.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "6.5.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "6.5.0",
    "babel-runtime": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.17.2",
    "chalk": "^2.3.0",
    "connect-busboy": "^0.0.2",
    "cors": "^2.8.4",
    "coveralls": "^3.0.0",
    "dataloader": "^1.4.0",
    "dotenv": "^4.0.0",
    "express": "4.16.0",
    "express-jwt": "^5.3.0",
    "graphql-subscriptions": "^0.5.5",
    "graphql-tools": "4.0.3",
    "graphql-tools-types": "1.1.16",
    "javascript-fortuna": "^1.0.8",
    "jwt-simple": "^0.5.1",
    "moment": "^2.19.2",
    "otp.js": "^1.1.0",
    "passport": "^0.4.0",
    "pg": "7.4.1",
    "pg-hstore": "^2.3.2",
    "qrcode-svg": "^1.0.0",
    "sequelize": "^4.22.8",
    "sequelize-cli": "^5.4.0",
    "sequelize-replace-enum-postgres": "^1.5.0",
    "stripe": "^6.2.1",
    "subscriptions-transport-ws": "^0.9.4",
    "update-batcher": "^1.0.0",
    "uuid": "^3.2.1",
    "web-push": "^3.3.0",
    "winston": "^3.0.0-rc1",
    "winston-slacker": "^1.0.3",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "eslint": "4.9.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "6.0.2",
    "eslint-plugin-react": "7.4.0",
    "graphql": "14.0.2",
    "graphql-tag": "^2.6.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^6.0.0",
    "minimal-graphql": "^1.0.3",
    "nodemon": "^1.12.1",
    "prettier-eslint-cli": "^4.7.0",
    "supertest": "^3.0.0"
  },
  "peerDependencies": {
    "graphql": "^0.11.7"
  }
}
